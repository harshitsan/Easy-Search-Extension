{
  "manifest_version": 2,
  "name": "Harshitsan Suggestion Box",
  "version": "0.1",
  "permissions": [
         "storage"
       ],
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "css": ["main.css"],
    "js": ["content.js"]
  }
],
"browser_action": {
  "default_icon": "e.png"
},
"background": {
  "scripts": ["background.js"]
}
}
